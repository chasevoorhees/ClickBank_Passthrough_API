    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net5.0/cbapiserviceconsole.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            // "requireExactSource": false
        },
        {
            "name": "DBG .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net5.0/cbapiserviceconsole.dll",
            "args": ["CERT_MODE=DEV", "ENABLE_TESTSALE=true", "URL_LIST=http://*:5000;https://*:5001;", "CORS_LIST=http://127.0.0.1:5000 http://localhost:5000 https://localhost:5001 https://127.0.0.1:5001"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            // "requireExactSource": false
        }
    ]
}